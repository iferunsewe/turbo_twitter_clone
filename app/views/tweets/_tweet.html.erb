<%= turbo_frame_tag dom_id(tweet) do %>
  <div class="card card-body">
    <div><%= tweet.body %></div>

    <div class="mt-2">
      <p>
        <strong>Likes:</strong> (<%= tweet.likes_count %>)

        <strong>Retweets:</strong> (<%= tweet.retweets_count %>)
      </p>
    </div>
  </div>
<% end %>
